<template name="starsTemplate">
  <view class='stars-container'>
    <view class='star'>
      <block wx:for="{{stars}}" wx:for-item="i">
        <image wx:if="{{i}}" src='/images/stars_s.png'></image>
        <image wx:else src='/images/stars.png'></image>
      </block>
    </view>
    <text class='star-score'>{{score}}</text>
  </view>
</template>